unit Classe;

interface

type
  TCalc = class(TObject)
  private
    FBotao: String;
    FVisor: String;
    FSoma: String;
    FSubt: String;

  protected
    procedure setBotao (value: String);

  public
    procedure setSoma (v1: String); static;
    procedure setSubt (v1: String); static;

    property Botao:String read FBotao write TCalc.setBotao;
    property Visor:String read FVisor write FVisor;
    property Soma:String read FSoma write TCalc.setSoma;
    property Subt:String read FSubt write TCalc.setSubt;

  end;

implementation

end.
